{
  "name": "cerebral",
  "version": "0.0.0-semantically-released",
  "description": "A state controller with its own debugger",
  "main": "src/index.js",
  "scripts": {
    "pretest": "eslint tests",
    "test": "istanbul cover mocha -- --compilers js:babel-register tests",
    "test:watch": "mocha --compilers js:babel-register --watch tests",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Christian Alfoni <christianalfoni@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cerebral/cerebral.git"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "commitizen": "^2.5.0",
    "conventional-changelog": "0.0.17",
    "cz-customizable": "^2.7.0",
    "ghooks": "^1.0.3",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.2",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "^2.6.1",
    "webpack": "^1.13.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "dependencies": {
    "cerebral-model": "^0.1.1",
    "cerebral-model-immutable": "^0.1.4",
    "cerebral-operators": "^0.2.1"
  }
}
