{
  "name": "cerebral-monorepo",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build:demos": "node ./scripts/run.js prepublish demos",
    "checkdeps": "node ./scripts/check.dependencies.js --auto-fix",
    "commit": "git-cz",
    "commitmsg": "node ./node_modules/cz-customizable-ghooks/lib/index.js .git/COMMIT_EDITMSG",
    "coverage": "node ./scripts/run.js coverage",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "deploy": "cd packages/website/publisher && npm run ci",
    "format": "npm run lint -- --fix && prettier --write --with-node-modules \"**/*.md\" \"**/*.css\" \"**/*.json\"",
    "link": "node ./scripts/run.js link",
    "lint": "eslint \"**/*.js\" \"**/*.ts\" \"**/*.tsx\"",
    "postcoverage": "istanbul-combine -d coverage -p summary -r lcov -r html packages/**/coverage/*.json",
    "postinstall": "npm run setup",
    "postsetup": "npm run link && node ./scripts/run.js setup all",
    "precommit": "lint-staged",
    "predeploy": "node ./scripts/run.js prepublish demos",
    "pretest": "npm run checkdeps",
    "release": "babel-node ./scripts/release/index.js",
    "setup": "node ./scripts/run.js prepublish nodemos",
    "test": "node ./scripts/run.js prepublish nodemos && node ./scripts/run.js test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebral/cerebral.git"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "@cerebral/router": "next",
    "@cerebral/storage": "next",
    "@types/classnames": "^2.2.9",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chalk": "1.1.3",
    "chokidar": "3.2.1",
    "classnames": "2.2.6",
    "es6-error": "4.1.1",
    "eventemitter3": "2.0.2",
    "faye-websocket": "^0.11.3",
    "local-storage": "2.0.0",
    "match-media": "0.2.0",
    "page": "1.11.5",
    "prism": "4.1.2",
    "proxyquire": "1.7.11",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "todomvc-app-css": "2.1.0",
    "todomvc-common": "1.0.4",
    "universal-websocket-client": "1.0.1",
    "uuid": "3.3.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,css,json}": [
      "prettier --write --with-node-modules",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "devDependencies": {
    "@angular/common": "^8.2.9",
    "@angular/compiler": "^8.2.9",
    "@angular/core": "^8.2.9",
    "@angular/platform-browser": "^8.2.9",
    "@angular/platform-browser-dynamic": "^8.2.9",
    "@babel/preset-typescript": "^7.6.0",
    "@types/mocha": "^5.2.7",
    "@types/page": "1.8.0",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/uuid": "^3.4.5",
    "assert": "^2.0.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-inferno": "^6.1.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-watch": "^7.0.0",
    "commitizen": "^4.0.3",
    "concurrently": "^4.1.2",
    "core-js": "^3.2.1",
    "coveralls": "^3.0.6",
    "cross-env": "^6.0.3",
    "cz-customizable": "^6.2.0",
    "cz-customizable-ghooks": "^1.5.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "execa": "^2.0.4",
    "express": "^4.17.1",
    "firebase-server": "^1.0.2",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.8",
    "inferno": "^7.3.1",
    "inferno-shared": "^7.3.1",
    "istanbul-combine": "^0.3.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "lint-staged": "^9.4.1",
    "marksy": "^8.0.0",
    "mocha": "^6.2.1",
    "mock-socket": "^6.0.4",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-typescript": "^1.0.0",
    "preact": "^10.0.0",
    "prettier": "^1.18.2",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react-scripts": "^3.2.0",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^16.10.2",
    "shx": "^0.3.2",
    "string-template": "^1.0.0",
    "symlink-dir": "^3.1.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3",
    "typescript-definition-tester": "0.0.6",
    "typescript-eslint-parser": "^22.0.0",
    "vue": "^2.6.10",
    "xhr-mock": "^2.5.0",
    "yargs": "^14.0.0",
    "zone.js": "^0.10.2"
  }
}
