{
  "plugins": [
    "version-transform",
    ["babel-plugin-transform-builtin-extend", {
      "globals": ["Error"]
    }]
  ],
  "env": {
    "test": {
      "presets": [["es2015"]],
      "ignore": []
    },
    "cjs": {
      "presets": [["es2015"]],
      "ignore": [
        "**/*.test.js"
      ]
    },
    "es": {
      "presets": [["es2015", { "modules": false }]],
      "ignore": [
        "**/*.test.js"
      ]
    }
  },
}
