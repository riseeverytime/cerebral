{
  "env": {
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        [
          "babel-plugin-transform-builtin-extend",
          {
            "globals": ["Error"]
          }
        ]
      ],
      "ignore": []
    },
    "cjs": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        [
          "babel-plugin-transform-builtin-extend",
          {
            "globals": ["Error"]
          }
        ]
      ],
      "ignore": ["**/*.test.js"]
    },
    "es": {
      "presets": [["@babel/preset-env", { "targets": { "esmodules": true } }]],
      "plugins": [
        [
          "babel-plugin-transform-builtin-extend",
          {
            "globals": ["Error"]
          }
        ]
      ],
      "ignore": ["**/*.test.js"]
    }
  }
}
